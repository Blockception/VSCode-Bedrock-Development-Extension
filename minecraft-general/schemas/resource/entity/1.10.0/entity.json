                "oneOf": [
                  {
                    "type": "string"
                  },
                  {
                    "type": "object",
                    "additionalProperties": {
                      "type": "string",
                      "description": "A render controller",
                      "title": "Render controller"
                    }
                  }
                ]
              }
            },
            "scripts": {
              "additionalProperties": false,
              "type": "object",
              "description": "TODO description: scripts",
              "title": "Scripts",
              "properties": {
                "animate": {
                  "type": "array",
                  "title": "Animate schema",
                  "minItems": 1,
                  "items": {
                    "oneOf": [
                      {
                        "type": "string",
                        "description": "TODO description: oneOf[0]",
