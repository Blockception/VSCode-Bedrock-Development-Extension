            "type": "string",
            "enum": [
                "beach",
                "desert",
                "extreme_hills",
                "flat",
                "forest",
                "ice",
                "jungle",
                "mesa",
                "mushroom_island",
                "ocean",
                "plain",
                "river",
                "savanna",
                "stone_beach",
                "swamp",
                "taiga",
                "the_end",
                "the_nethe"
            ]
        }
    }
}