{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "minecraft.behaviour.entities.1.8.0.filters.has_ability",
    "type": "object",
    "title": "Filters has_ability 1.8.0",
    "additionalProperties": false,
    "description": "Returns true when the subject entity has the named ability.",
    "required": [ "value" ],
    "properties": {
        "operator": {
            "$ref": "base_operator.json"
        },
        "subject": {
            "$ref": "base_subject.json"
        },
        "value": {
            "$ref": "base_value.json",
            "type": "string",
            "description": "(Required) The Ability type to test",
            "enum": [
                "flySpeed",
                "flying",
                "instabuild",
                "invulnerable",
                "lightning",
                "mayfly",
                "mute",
                "noclip",
                "walkSpeed",
                "worldbuilder"
            ]
        }
    }
}