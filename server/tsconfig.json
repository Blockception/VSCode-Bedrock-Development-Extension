{
  "compilerOptions": {
    "esModuleInterop": true,
    "lib": ["ES2019"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "outDir": "out",
    "resolveJsonModule": true,
    "rootDir": "src",
    "sourceMap": true,
    "strict": true,
    "target": "es2019"
  },
  "exclude": ["node_modules", ".vscode-test", "*.test.ts"],
  "include": ["src"],
  "references": []
}
