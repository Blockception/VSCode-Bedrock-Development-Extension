{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "Version",
    "type": "object",
    "title": "Command Schema",
    "definitions": {
        "commands": {
            "type": "array",
            "title": "Commands",
            "items": {
                "additionalProperties": false,
                "title": "Command",
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "documentation": {
                        "additionalProperties": false,
                        "type": "object",
                        "properties": {
                            "kind": {
                                "type": "string",
                                "enum": [
                                    "plaintext",
                                    "markdown"
                                ]
                            },
                            "value": {
                                "type": "string"
                            }
                        }
                    },
                    "parameters": {
                        "type": "array",
                        "items": {
                            "type": "object",
                            "properties": {
                                "Text": {
                                    "type": "string"
                                },
                                "Type": {
                                    "type": "string",
                                    "enum": [
                                        "block",
                                        "boolean",
                                        "command",
                                        "coordinate",
                                        "effect",
                                        "entity",
                                        "event",
                                        "function",
                                        "float",
                                        "gamemode",
                                        "integer",
                                        "integerTest",
                                        "item",
                                        "jsonItem",
                                        "jsonRawText",
                                        "keyword",
                                        "locateFeature",
                                        "objective",
                                        "operation",
                                        "replaceMode",
                                        "selector",
                                        "selectorPlayer",
                                        "slotType",
                                        "slotID",
                                        "sound",
                                        "string",
                                        "target",
                                        "tag",
                                        "tickingarea",
                                        "unknown",
                                        "xp"
                                    ]
                                },
                                "Required": {
                                    "type": "boolean"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "properties": {
        "vanilla": {
            "$ref": "#/definitions/commands"
        },
        "edu": {
            "$ref": "#/definitions/commands"
        }
    }
}